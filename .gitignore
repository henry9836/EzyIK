ExportedObj/
Project/.consulo/
Project/*.csproj
Project/*.unityproj
Project/*.sln
Project/*.suo
Project/*.tmp
Project/*.user
Project/*.userprefs
Project/*.pidb
Project/*.booproj
Project/*.svd
Project/*.pdb
Project/*.mdb
Project/*.opendb
Project/*.VC.db

# Unity3D generated meta files
Project/*.pidb.meta
Project/*.pdb.meta
Project/*.mdb.meta

# Unity3D generated file on crash reports
Project/sysinfo.txt

# Builds
Project/*.apk
Project/*.unitypackage

# Crashlytics generated file
Project/crashlytics-build.properties

